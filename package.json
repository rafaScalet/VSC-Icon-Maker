    "vscode",
    "vscode-utils",
    "vscode-icons",
    "icons",
    "icon-maker"
  ],
  "author": "rafascalet",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.10.7",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
